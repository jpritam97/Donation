<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.donation">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Donation"
        tools:targetApi="31">
        <activity
            android:name=".my_history"
            android:exported="true" >

        </activity>
        <activity
            android:name=".my_account"
            android:exported="true" >

        </activity>
        <activity
            android:name=".NavigationbarActivity"
            android:exported="true"
            android:label="@string/title_activity_navigationbar"
            android:theme="@style/Theme.Donation.NoActionBar" >

        </activity>
        <activity
            android:name=".Form"
            android:exported="true" >

        </activity>

        <activity
            android:name=".Front_page"
            android:exported="false" />
        <activity
            android:name=".Home_Screen"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".Splash_screen"
            android:exported="false" />
        <activity
            android:name=".Navigationbar"
            android:exported="false"
            android:label="@string/title_activity_navigationbar"
            android:theme="@style/Theme.Donation.NoActionBar" />
        <activity
            android:name=".send_otp"
            android:exported="false" />
        <activity
            android:name=".otp_verification"
            android:exported="false" />
        <activity
            android:name=".Login"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>